pipeline {
    agent any
    tools {
         maven 'Maven3'
    }
    stages {
      stage ('Clone') {
            steps {
               git branch: 'main', url: 'https://github.com/prethivirajp/jfrog-assesment.git'
            }
        }
        stage ('Compile') {
            steps {
               //sh 'mvn help:effective-settings'
               sh 'mvn clean compile'
            }
        }
    }
}
